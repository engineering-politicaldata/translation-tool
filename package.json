{
    "name": "translation-tool",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "init-prepare-commit-message": "cp ./prepare-commit-msg ./.git/hooks",
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "format-tsx": "prettier --write \"./**/*.tsx\"",
        "format-ts": "prettier --write \"./**/*.ts\"",
        "build:analyze": "ANALYZE=true yarn build",
        "migration": "yarn && knex migrate:latest",
        "migration:new": "yarn && knex migrate:make $1"
    },
    "dependencies": {
        "@material-ui/core": "^4.11.3",
        "@material-ui/icons": "^4.11.2",
        "axios": "^0.21.1",
        "bcrypt": "^5.0.1",
        "classnames": "^2.3.1",
        "cookie": "^0.4.1",
        "cors": "^2.8.5",
        "delete-all-cookies": "^0.3.0",
        "formik": "^2.2.6",
        "js-cookie": "^3.0.1",
        "jsonwebtoken": "^8.5.1",
        "knex": "^0.95.11",
        "next": "10.1.3",
        "pg": "^8.7.1",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "sha256": "^0.2.0",
        "styled-components": "^5.2.3",
        "swr": "^1.0.1",
        "yup": "^0.32.9"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "^12.0.1",
        "@types/bcrypt": "^5.0.0",
        "@types/cookie": "^0.4.1",
        "@types/cors": "^2.8.12",
        "@types/js-cookie": "^3.0.0",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/node": "^15.12.2",
        "@types/react": "^17.0.11",
        "@types/yup": "^0.29.11",
        "babel-plugin-import": "^1.13.3",
        "husky": "4",
        "lint-staged": "^10.5.4",
        "prefix-commit-message": "^3.0.0",
        "prettier": "^2.2.1",
        "typescript": "^4.3.2"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{tsx,ts,js,json,css,md}": [
            "prettier --write",
            "git add"
        ]
    }
}
